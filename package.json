{
  "name": "@spillay/usercontrolmanager",
  "version": "1.0.4",
  "description": "User Controls to Manage Users and Permissions",
  "main": "dist/index.js",
  "contributors": [
    {
      "name": "Suresh Pillay",
      "email": "me@sureshpillay.com"
    },
    {
      "name": "Bhaskar Veligeti",
      "email": "bhaskarv20@gmail.com"
    }
  ],
  "scripts": {
    "webpack-watch": "webpack --mode development --watch",
    "start": "nodemon -e src/styles/ -x \"npm run sass\" --exec 'npm run webpack-watch'",
    "build": "npm run prebuild && npm run sass && webpack --mode production",
    "dev": "webpack --mode development && npm run build:tests",
    "build:tests": "rimraf test-dist/ && babel test/ --out-dir test-dist/",
    "testold": "node node_modules/mocha/bin/mocha --compilers js:babel-core/register --reporter spec test-dist/*",
    "test": "jest",
    "clean": "rimraf dist/ && rimraf test-dist/",
    "sass:dist-min": "node-sass  --output-style=compressed --importer=node_modules/node-sass-tilde-importer src/styles/styles.scss > dist/css/usercontrolmanager.min.css",
    "sass:dist-min-loginView": "node-sass  --output-style=compressed --importer=node_modules/node-sass-tilde-importer src/styles/loginView.scss > dist/css/loginView.min.css",
    "sass:dist": "node-sass  --importer=node_modules/node-sass-tilde-importer src/styles/styles.scss > dist/css/usercontrolmanager.css",
    "sass": "npm run sass:dist-min && npm run sass:dist && npm run sass:dist-min-loginView",
    "prebuild": "mkdirp ./dist && mkdirp ./dist/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spillay/UserControlManager.git"
  },
  "keywords": [
    "React",
    "Access Control",
    "Roles",
    "User Management"
  ],
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-replace-object-assign": "^1.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "3.5.0",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
