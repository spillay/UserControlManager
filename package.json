    "mkdirp": "^0.5.1",
    "mocha": "3.2.0",
    "node-sass": "^4.9.0",
    "node-sass-tilde-importer": "^1.0.2",
    "nodemon": "^1.17.4",
    "react-test-renderer": "^16.3.2",
    "rimraf": "2.5.4",
    "run-sequence": "1.2.2",
    "webpack-cli": "^2.1.3"
  },
  "engine": "node >= 0.10.x",
  "author": "Suresh Pillay",
  "license": "ISC",
  "bugs": {
